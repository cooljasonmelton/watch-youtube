{
  "name": "watch youtube",
  "manifest_version": 3,

  "version": "1.0",
  "description": "open youtube video in an embedded player",
  "permissions": ["scripting", "activeTab", "tabs"],
  "action": {
    "default_icon": {},
    "default_title": "open video",
    "default_popup": "popup.html"
  },
  "host_permissions": ["*://*/*", "<all_urls>"],
  "icons": {}
}
